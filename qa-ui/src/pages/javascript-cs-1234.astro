---
import Layout from '../layouts/Layout.astro';
import Course from '../components/Course.astro';

const title = 'Javascript CS-1234';
const courseId = 6;
const details = 'Javascript CS-1234 Course Section';
const img =
  'https://craftypixels.com/placeholder-image/200x80/ffff00/595959&text=Javascript+CS-1234';
const slug = 'javascript-cs-1234';
---

<Layout title={title}>
  <Course
    courseId={courseId}
    courseTitle={title}
    courseDetails={details}
    courseImg={img}
    courseSlug={slug}
  />
</Layout>
