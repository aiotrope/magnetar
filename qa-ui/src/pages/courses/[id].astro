---
export const prerender = true;

export const getStaticPaths = () => {
  return [
    { params: { id: '1' } },
    { params: { id: '2' } },
    { params: { id: '3' } },
    { params: { id: '4' } },
    { params: { id: '5' } },
    { params: { id: '6' } },
    { params: { id: '7' } },
    { params: { id: '8' } },
    { params: { id: '9' } },
    { params: { id: '10' } },
  ];
};

import Layout from '../../layouts/Layout.astro';
import Course from '../../components/Course.svelte';

const id = Astro.params.id;

let linkId = parseInt(id);
---

<Layout title={`Course #: ${linkId}`}>
  <Course courseId={linkId} client:only="svelte" />
</Layout>
