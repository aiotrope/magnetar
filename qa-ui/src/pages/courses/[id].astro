---
export const prerender = true;

export const getStaticPaths = () => {
  return [
    { params: { id: '1' } },
    { params: { id: '2' } },
    { params: { id: '3' } },
    { params: { id: '4' } },
    { params: { id: '5' } },
    { params: { id: '6' } },
    { params: { id: '7' } },
    { params: { id: '8' } },
  ];
};

import Layout from '../../layouts/Layout.astro';

const id = Astro.params.id;
---

<Layout>
  <p>You're in id {id}</p>
</Layout>
